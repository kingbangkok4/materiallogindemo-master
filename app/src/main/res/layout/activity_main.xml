<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:fitsSystemWindows="true">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/textView2"
        android:layout_alignTop="@+id/textView2"
        android:background="#f1e41206"
        android:textColor="#fffefe" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="22dp"
        android:layout_toEndOf="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:text="Notifications"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:text="Logout"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_marginTop="29dp"
        android:layout_toEndOf="@+id/textView8"
        android:layout_toLeftOf="@+id/textView9"
        android:layout_toRightOf="@+id/textView8"
        android:textColor="#040404"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignTop="@+id/textView6"
        android:text="Feed"
        android:textColor="#040404"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView6"
        android:background="@drawable/user" />

    <Button
        android:id="@+id/btnNotification"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/textView8"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignTop="@+id/button2"
        android:background="@drawable/nat" />


    <Button
        android:id="@+id/btnLogout"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/button2"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:background="@drawable/log" />

    <Button
        android:id="@+id/btnFeed"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/textView8"
        android:layout_toLeftOf="@+id/textView7"
        android:layout_toStartOf="@+id/textView7"
        android:background="@drawable/feed" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView8"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:text="ชื่อ - นามสกุล"
        android:textColor="#040404"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="10dp"
        android:text="Start (ต้นทาง)"
        android:textColor="#040404" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtStart"
        android:layout_alignStart="@+id/txtStart"
        android:layout_below="@+id/txtStart"
        android:layout_marginTop="5dp"
        android:text="End (ปลายทาง)"
        android:textColor="#040404" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtEnd"
        android:layout_alignStart="@+id/txtEnd"
        android:layout_below="@+id/button7"
        android:layout_marginTop="5dp"
        android:text="จุดนัดพบ"
        android:textColor="#040404" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="45dp"
        android:text="Time"
        android:textColor="#040404" />

    <Button
        android:id="@+id/btnSearch"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/textView12"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:background="@drawable/search" />

    <Button
        android:id="@+id/button7"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/btnSearch"
        android:layout_alignStart="@+id/btnSearch"
        android:layout_below="@+id/textView12"
        android:background="@drawable/ccar" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView14"
        android:layout_below="@+id/textView16"
        android:layout_marginTop="45dp"
        android:text="License Plate"
        android:textColor="#040404" />

    <EditText
        android:id="@+id/txtStart"
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="5dp"
        android:layout_toLeftOf="@+id/radio"
        android:layout_toStartOf="@+id/radio"
        android:background="#ffffff"
        android:textColor="#030303" />

    <EditText
        android:id="@+id/txtEnd"
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/button7"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:layout_toLeftOf="@+id/radio"
        android:layout_toStartOf="@+id/radio"
        android:background="#ffffff"
        android:textColor="#030303" />

    <EditText
        android:id="@+id/txtAppoint"
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/txtEnd"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignRight="@+id/txtEnd"
        android:layout_alignStart="@+id/textView14"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="5dp"
        android:background="#ffffff"
        android:textColor="#030303" />

    <RadioGroup
        android:id="@+id/radio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btnFeed"
        android:layout_alignStart="@+id/btnFeed"
        android:layout_alignTop="@+id/txtAppoint">

        <RadioButton
            android:id="@+id/radioCar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/button7"
            android:layout_alignRight="@+id/button7"
            android:layout_below="@+id/textView14"
            android:checked="true"
            android:text="มีรถ"
            android:textColor="#040404" />

        <RadioButton
            android:id="@+id/radioNoCar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/radioCar"
            android:layout_alignStart="@+id/radioCar"
            android:layout_below="@+id/radioCar"
            android:checked="false"
            android:text="ไม่มีรถ"
            android:textColor="#030303" />
    </RadioGroup>

    <EditText
        android:id="@+id/txtLicensePlate"
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/btnTime"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignRight="@+id/btnTime"
        android:layout_alignStart="@+id/textView18"
        android:layout_below="@+id/textView18"
        android:layout_marginTop="5dp"
        android:background="#ffffff"
        android:textColor="#030303" />

    <EditText
        android:id="@+id/txtTime"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btnSave"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignRight="@+id/btnSave"
        android:layout_alignStart="@+id/textView18"
        android:layout_below="@+id/textView16"
        android:ems="10"
        android:enabled="false"
        android:inputType="time"
        android:textColor="#080808" />

    <Button
        android:id="@+id/btnTime"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/txtTime"
        android:layout_alignEnd="@+id/txtAppoint"
        android:layout_alignRight="@+id/txtAppoint"
        android:layout_toEndOf="@+id/btnSave"
        android:layout_toRightOf="@+id/btnSave"
        android:text="..." />

    <Button
        android:id="@+id/btnSave"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@drawable/green"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_marginBottom="11dp" />

    <Button
        android:id="@+id/btnCancel"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@drawable/close"
        android:layout_alignTop="@+id/btnSave"
        android:layout_toRightOf="@+id/btnSave"
        android:layout_toEndOf="@+id/btnSave" />

</RelativeLayout>
